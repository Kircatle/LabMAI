   "source": [
    "import numpy as np\n",
    "\n",
    "def f(x):\n",
    "    return 1/np.sqrt(2*np.pi)*np.exp(-(x*x)/2)\n",
    "\n",
    "def tr_integral(f,xmin,xmax,n):\n",
    "    dx=(xmax-xmin)/n\n",
    "    area=0\n",
    "    x=xmin\n",
    "    for i in range(n):\n",
    "        area+=dx*(f(x)+f(x+dx))/2\n",
    "        x+=dx\n",
    "    return area\n"
   ]
  },
